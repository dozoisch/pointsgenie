    "git-rev": "0.2.x",
    "glob": "~5.0.5",
    "koa": "~0.19.1",
    "koa-bodyparser": "~1.5.0",
    "koa-compress": "1.0.x",
    "koa-error": "1.1.x",
    "koa-generic-session": "~1.8.0",
